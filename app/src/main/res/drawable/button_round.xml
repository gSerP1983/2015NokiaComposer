<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape android:shape="oval">
            <solid android:color="@color/nokiaBtn"/>
            <stroke
                android:width="1dp"
                android:color="@color/nokiaBtn"
                />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/nokiaScreen"/>
            <stroke
                android:width="1dp"
                android:color="@color/nokiaScreen"
                />
        </shape>
    </item>
</selector>
